{
  "layout": "A4 landscape",
  "outputFormat": "pdf",
  "attributes": {
    "map": {
      "projection": "EPSG:4326",
      "dpi": 254,
      "center": [
        -95.8506355,
        37.163851
      ],
      "longitudeFirst": true,
      "scale": 45000000,
      "layers": [
        {
          "type": "grid",
          "gridType": "points",
          "numberOfLines": [5,5],
          "renderAsSvg": true,
          "haloColor" : "#CCFFCC", // most css color definitions supported
          "labelColor" : "black",
          "labelFormat": "%1.0f %s",
          "indent": 10,
          "haloRadius" : 4,
          "font" : {
            "name" : ["Arial", "Helvetica", "Nimbus Sans L", "Liberation Sans", "FreeSans", "Sans-serif"],
            "size" : 8,
            "style" : "BOLD"
          }
        },
        {
          "type": "WMS",
          "layers": ["topp:states"],
          "baseURL": "http://localhost:8080/gs-web-app/wms",
          "imageFormat": "image/png",
          "version": "1.3.0"
        }
      ]
    }
  }
}