    <bean id="wmsLayerParser" class="org.mapfish.print.map.image.wms.WmsLayerFactoryPlugin" />
    <bean id="tiledWmsLayerParser" class="org.mapfish.print.map.tiled.wms.TiledWmsLayerParserPlugin" />
