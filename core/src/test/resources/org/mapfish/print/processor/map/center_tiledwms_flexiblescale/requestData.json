{
    "layout": "A4 landscape",
    "outputFormat": "pdf",
    "attributes": {
        "map": {
            "projection": "CRS:84",
            "dpi": 72,
            "rotation": 0,
            "center": [-95, 35],
            "longitudeFirst": true,
            "scale": 30000000,
            "layers": [
                {
                    "type": "geojson",
